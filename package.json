{
  "name": "angular-lazyload-webpack",
  "version": "1.0.0",
  "description": "Lazy load AngularJS modules using Webpack and AngularUI Router",
  "scripts": {
    "start": "hs build",
    "build": "webpack -p",
    "devs": "webpack-dev-server --content-base build --hot --inline --devtool=inline-source-map",
    "test": "karma start test/karma.config.js"
  },
  "author": "Michal Zalecki <michal@michalzalecki.com>",
  "license": "MIT",
  "dependencies": {
    "angular": "^1.6.5",
    "angular-route": "^1.6.5",
    "angular-ui-router": "^1.0.3",
    "oclazyload": "^1.1.0"
  },
  "devDependencies": {
    "angular-mocks": "^1.4.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-angularjs-annotate": "^0.8.0",
    "babel-preset-env": "^1.6.0",
    "http-server": "^0.10.0",
    "jasmine-core": "^2.3.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-html-reporter": "^0.2.6",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^2.0.4",
    "node-libs-browser": "^2.0.0",
    "phantomjs": "^2.1.7",
    "raw-loader": "^0.5.1",
    "webpack": "^3.5.4",
    "webpack-dev-server": "^2.7.1"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "angularjs-annotate",
      "add-module-exports"
    ]
  }
}
